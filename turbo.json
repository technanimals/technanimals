{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "build/**"]
    },
    "test": {
      "dependsOn": ["^build"]
    },
    "test:once": {
      "dependsOn": ["^build"]
    },
    "dev": {
      "cache": false
    }
  }
}
