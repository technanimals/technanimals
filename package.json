{
  "name": "@technanimals/technanimals",
  "version": "0.0.7",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/technanimals/technanimals"
  },
  "workspaces": [
    "packages/*",
    "packages/config/*",
    "packages/paystack/*",
    "packages/paystack/core/*",
    "packages/paystack/core/resources/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "lint-stage": "lint-staged",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky install",
    "release": "release-it --ci",
    "test": "turbo run test",
    "test:once": "turbo run test:once"
  },
  "devDependencies": {
    "@storybook/react": "~6.4.22",
    "@technanimals/eslint-plugin": "*",
    "@types/jest": "~27.4.1",
    "@types/uuid": "~8.3.4",
    "eslint": "~8.14.0",
    "graphql": "~16.4.0",
    "graphql-tag": "~2.12.6",
    "husky": "~7.0.4",
    "jest": "~27.5.1",
    "jest-transform-graphql": "~2.1.0",
    "lint-staged": "~12.4.1",
    "prettier": "~2.6.2",
    "react-native-builder-bob": "~0.18.2",
    "release-it": "~14.14.2",
    "release-it-yarn-workspaces": "~2.0.1",
    "ts-jest": "~27.1.4",
    "ts-node": "~10.7.0",
    "turbo": "latest",
    "typescript": "~4.6.4"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@1.22.18",
  "dependencies": {
    "react": "17.0.2",
    "react-native": "0.68.2"
  }
}
