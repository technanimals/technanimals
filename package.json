{
  "name": "@technanimals/transformers",
  "version": "0.0.3",
  "private": true,
  "workspaces": [
    "packages/*",
    "packages/config/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
